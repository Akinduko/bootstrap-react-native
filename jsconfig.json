{
  "compilerOptions": {
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "<assets>/*": [
        "./src/assets/*"
      ],
      "<components>/*": [
        "./src/components/*"
      ],
      "<config>/*": [
        "./src/config/*"
      ],
      "<constants>/*": [
        "./src/constants/*"
      ],
      "<integrations>/*": [
        "./src/integrations/*"
      ],
      "<navigation>/*": [
        "./src/navigation/*"
      ],
      "<screens>/*": [
        "./src/screens/*"
      ],
      "<services>/*": [
        "./src/services/*"
      ],
      "<state>/*": [
        "./src/state/*"
      ],
      "<utils>/*": [
        "./src/utils/*"
      ],
    }
  },
  "exclude": [
    "node_modules"
  ]
}
